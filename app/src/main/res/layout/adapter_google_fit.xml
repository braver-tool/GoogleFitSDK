<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".MainActivity">

    <androidx.cardview.widget.CardView
        android:id="@+id/gFitDataAdapterCardView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="12sp"
        android:paddingEnd="12sp"
        app:cardCornerRadius="5dp"
        app:cardUseCompatPadding="true"
        app:cardBackgroundColor="#fff"
        app:cardElevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/nameDateLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:foreground="?attr/selectableItemBackgroundBorderless"
                tools:ignore="UnusedAttribute">

                <TextView
                    android:id="@+id/moduleTitleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:fontFamily="@font/akaya_telivigala"
                    android:text="Blood Pressure"
                    android:textColor="@color/grey_six"
                    android:textSize="@dimen/_15sdp" />

                <TextView
                    android:id="@+id/dateTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:fontFamily="@font/akaya_telivigala"
                    android:text="2022-04-12 12:23:11"
                    android:textAllCaps="false"
                    android:textColor="@color/grey_six"
                    android:textSize="@dimen/_12sdp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginBottom="@dimen/_3sdp">

                <LinearLayout
                    android:id="@+id/valueOneLinearLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_4sdp">

                    <TextView
                        android:id="@+id/valueOneTitleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/akaya_telivigala"
                        android:text="SYS"
                        android:textColor="#21ad25"
                        android:textSize="@dimen/_14sdp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/valueOneDataTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:text="123"
                            android:textColor="@color/grey_three"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/valueOneMeasureTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:paddingStart="@dimen/_2sdp"
                            android:paddingEnd="@dimen/_1sdp"
                            android:text="mmHg"
                            android:textColor="@color/grey_six"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/valueTwoLinearLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:layout_toEndOf="@+id/valueOneLinearLayout"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_4sdp">

                    <TextView
                        android:id="@+id/valueTwoTitleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/akaya_telivigala"
                        android:text="DIA"
                        android:textColor="#e36b18"
                        android:textSize="@dimen/_14sdp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/valueTwoDataTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:text="98"
                            android:textColor="@color/grey_three"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/valueTwoMeasureTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:paddingStart="@dimen/_2sdp"
                            android:paddingEnd="@dimen/_1sdp"
                            android:text="mmHg"
                            android:textColor="@color/grey_six"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/valueThreeLinearLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:layout_toEndOf="@+id/valueTwoLinearLayout"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_4sdp">

                    <TextView
                        android:id="@+id/valueThreeTitleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/akaya_telivigala"
                        android:text="Pulse"
                        android:textColor="@color/purple_200"
                        android:textSize="@dimen/_14sdp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/valueThreeDataTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:text="65"
                            android:textColor="@color/grey_three"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/valueThreeMeasureTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:paddingStart="@dimen/_2sdp"
                            android:paddingEnd="@dimen/_1sdp"
                            android:text="Beats/min"
                            android:textColor="@color/grey_six"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/valueFourLinearLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:layout_toEndOf="@+id/valueThreeLinearLayout"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_2sdp"
                    android:paddingEnd="@dimen/_4sdp">

                    <TextView
                        android:id="@+id/valueFourTitleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/akaya_telivigala"
                        android:text="REM"
                        android:textColor="@color/yellow"
                        android:textSize="@dimen/_14sdp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/valueFourDataTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:text="65"
                            android:textColor="@color/grey_three"
                            android:textSize="@dimen/_14sdp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/valueFourMeasureTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/akaya_telivigala"
                            android:paddingStart="@dimen/_2sdp"
                            android:paddingEnd="@dimen/_1sdp"
                            android:text="Beats/min"
                            android:textColor="@color/grey_six"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>